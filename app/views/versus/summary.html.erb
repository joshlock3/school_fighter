
<section>
    <div class="container">

      <h2 class="message"><%= @final_result_message %></h2>

      <div class="summary">

        <div class="half">

          <h3><%= @my_school.name %></h3>
          <% @attrs[0].each do |k, v| %>
            <div><%= "#{k}: #{v}" %></div>
            </br>
          <% end %>

        </div>

        <div class="half">

          <h3><%= @opp_school.name %></h3>
          <% @attrs[1].each do |k, v| %>
            <div><%= "#{k}: #{v}" %></div>
            </br>
          <% end %>

        </div>

        <div class="clearfix"></div>

        <a href="" class="coaches"></a>

      </div>

    </div>
</section>
